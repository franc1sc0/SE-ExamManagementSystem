@model IEnumerable<ExamManagementSystem.Models.RegExam>

@{
    ViewData["Title"] = "EnterResults";
}

<h2>EnterResults</h2>


    <table class="table">
        <tr>

            <th>
                TxStateID
            </th>

            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>


                <td>
                    @Html.DisplayFor(modelItem => item.Student.txStateID)
                </td>
                <td>

            
                 <form asp-action="Submit" method="post">
                    <input name="val"class="form-control" />
                    <input name="id" hidden="hidden" value=@item.regExamID />
                    <input type="submit" value="Save" class="btn btn-default" />
                 </form>
                    
</td>

            </tr>
        }
    </table>


